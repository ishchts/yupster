{
  "name": "yupster",
  "type": "module",
  "version": "0.0.1",
  "description": "Лёгкая и мощная библиотека валидации данных, вдохновлённая Yup",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ishchts/yupster.git"
  },
  "scripts": {
    "build": "yarn rollup -c",
    "dev": "rollup -c -w",
    "watch": "yarn tsx watch ./src/index.ts",
    "lint": "yarn eslint .",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "packageManager": "yarn@3.8.0",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@rollup/plugin-commonjs": "28.0.2",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^22.13.4",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.20.1",
    "globals": "^16.0.0",
    "rollup": "4.30.1",
    "rollup-plugin-delete": "2.1.0",
    "tslib": "2.8.1",
    "tsx": "4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.8"
  }
}
